#Getting numbers
print("Enter x to exit")
list1 = []
while 1:
    n = input("Enter a number: ")
    if n == 'x':
        break
    else:
        list1.append(int(n))
print(list1)

#operation
for i in list1:
    if i % 20 == 0:
        print("The numbers divisible by 20 are: " + str(i))
    else:
        print("The numbers not divisble by 20 are: " + str(i))
